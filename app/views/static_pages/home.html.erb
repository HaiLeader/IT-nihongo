<div class="container">
  <div class="row">

    <div class="col-md-3">
      <p class="lead"><%= t "user.musical_instrument_types" %></p>
      <div class="list-group">
        <% @musical_instruments.each do |musical_instrument| %>
          <%= link_to musical_instrument.name, "#",
          class: "list-group-item", data: {id: musical_instrument.id} %>
        <% end %>
      </div>
    </div>
    <div class="home-content">
      <%= render "home_page_content" %>
    </div>
  </div>
</div>
