<div class="col-md-8 col-md-offset-2">
  <%= form_for [:admin, @musical_instrument] do |f| %>
    <%= render "shared/error_messages", object: @musical_instrument %>

    <div class="form-group">
      <%= f.label :name, t("admin.musical_instrument.musical_instrument_name") %>
      <%= f.text_field :name, class: "form-control" %>
    </div>

    <div class="form-group">
      <%= f.label :price, t("admin.musical_instrument.price") %>
      <%= f.number_field :price, class: "form-control" %>
    </div>

    <div class="form-group">
      <%= f.label :material, t("admin.musical_instrument.material") %>
      <%= f.text_field :material, class: "form-control" %>
    </div>

    <div class="form-group">
      <%= f.label :sound_quality, t("admin.musical_instrument.sound_quality") %>
      <%= f.text_field :sound_quality, class: "form-control" %>
    </div>

    <div class="form-group">
      <%= f.label :felling, t("admin.musical_instrument.felling") %>
      <%= f.text_field :felling, class: "form-control" %>
    </div>

      <div class="form-group">
      <%= f.label :accessories, t("admin.musical_instrument.accessories") %>
      <%= f.text_field :accessories, class: "form-control" %>
    </div>

      <div class="form-group">
      <%= f.label :brand, t("admin.musical_instrument.brand") %>
      <%= f.text_field :brand, class: "form-control" %>
    </div>

      <div class="form-group">
      <%= f.label :seller, t("admin.musical_instrument.seller") %>
      <%= f.text_field :seller, class: "form-control" %>
    </div>
    <div class="row">
      <div class="actions form-group center text-inline">
        <%= f.submit yield(:submit), class: "btn btn-primary" %>
        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      </div>
    </div>
  <% end %>
</div>
